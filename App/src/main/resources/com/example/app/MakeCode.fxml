<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@HomePage.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.app.MakeCodeController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <left>
            <VBox fx:id="VBox1" prefHeight="200.0" prefWidth="100.0"   BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#onStatsButtonclick" prefHeight="26.0" prefWidth="101.0" text="STATS" textFill="WHITE" />
                  <Button mnemonicParsing="false" onAction="#onProducedButtonClick" prefHeight="26.0" prefWidth="101.0"  text="PRODUCERET" textFill="WHITE" />
                  <Button mnemonicParsing="false" onAction="#onScanButtonClick"  prefHeight="26.0" prefWidth="101.0"  text="SCAN" textFill="WHITE" />
                  <Button mnemonicParsing="false" onAction="#onReadyButtonClick" prefHeight="26.0" prefWidth="101.0"  text="READY" textFill="WHITE" />
                  <Button mnemonicParsing="false" onAction="#onProfileButtonClick" prefHeight="26.0" prefWidth="101.0"  text="PROFIL" textFill="WHITE" />
               </children>
            </VBox>
         </left>
         <top>
            <HBox fx:id="HBox1" prefHeight="50.0" prefWidth="600.0"  BorderPane.alignment="CENTER" />
         </top>
         <center>
            <Pane fx:id="mainPane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
            <Text fx:id="myText" />

         </center>
      </BorderPane>
   </children>
</AnchorPane>
